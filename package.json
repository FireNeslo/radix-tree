{
  "name": "radix-tree",
  "version": "0.1.0",
  "description": "Node.js version of a radix tree usable for routers or url path based storage.",
  "main": "index.js",
  "scripts": {
    "pretest": "babel . --extensions .es6 --out-dir .",
    "precoverage": "babel . --extensions .es6 --out-dir .",
    "precoveralls": "babel . --extensions .es6 --out-dir .",
    "prebench": "babel . --extensions .es6 --out-dir .",
    "test": "mocha --harmony test --recursive",
    "coverage": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha",
    "coveralls": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha && cat ./coverage/lcov.info | coveralls",
    "bench": "node --harmony benchmark/joined.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evangelion1204/radix-tree.git"
  },
  "dependencies": {
    "chai": "^3.4.1",
    "lodash": "^3.10.1"
  },
  "author": "Michael Iwersen <mi.iwersen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evangelion1204/radix-tree/issues"
  },
  "homepage": "https://github.com/evangelion1204/radix-tree#readme",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.2.0",
    "babel-plugin-transform-es2015-parameters": "^6.1.18",
    "benchmark": "^1.0.0",
    "coveralls": "^2.11.6",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  }
}
